    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "esbuild": "0.19.12",
    "eslint": "^8.20.0",
    "eslint-plugin-hydrogen": "0.12.2",
    "prettier": "^2.8.4",
    "typescript": "^5.2.2",
