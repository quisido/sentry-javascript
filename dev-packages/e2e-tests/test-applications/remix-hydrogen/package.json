    "@shopify/hydrogen": "2024.4.5",
    "@shopify/remix-oxygen": "^2.0.4",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^3.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
